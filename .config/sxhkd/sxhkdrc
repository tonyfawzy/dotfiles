super + Return
	$TERMINAL

super + {equal, minus, BackSpace}
	$HOME/script/volume.sh {up, down, toggle}


super + shift + f
	$TERMINAL lf

super + w
	$BROWSER

super + d
	dmenu_run
super + Print
	$HOME/script/screencast.sh

Print
	$HOME/script/screenshot.sh

Scroll_Lock
	pkill -RTMIN+20 dwmblocks

super + Escape
	pkill -USR1 -x sxhkd
